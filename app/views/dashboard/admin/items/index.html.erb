<p id="notice"><%= notice %></p>

<h1>Listing Dashboard Admin Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dashboard_admin_items.each do |dashboard_admin_item| %>
      <tr>
        <td><%= link_to 'Show', dashboard_admin_item %></td>
        <td><%= link_to 'Edit', edit_dashboard_admin_item_path(dashboard_admin_item) %></td>
        <td><%= link_to 'Destroy', dashboard_admin_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item', new_dashboard_admin_item_path %>
